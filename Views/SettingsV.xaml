<UserControl x:Class="WhereIsMyData.Views.SettingsV"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WhereIsMyData.Views"
             mc:Ignorable="d" 
             Background="#f1f1f1"
             d:DesignHeight="400" d:DesignWidth="650">
    <Grid Background="White" Margin="32">
        <StackPanel Orientation="Vertical">
            <Button Grid.Row="1" Width="100" Height="20" Content="Reset All Data" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="32,32" Command="{Binding ResetBtn}"/>
        </StackPanel>
    </Grid>
</UserControl>
